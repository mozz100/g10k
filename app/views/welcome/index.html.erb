<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>
<div ng-app="g10kApp">
    <div ng-controller="RunnersCtrl">
        <p ng-hide="runners.length > 0">Loading...</p>
        <table border="1" ng-show="runners.length > 0">
            <tr>
                <th>Name</th>
                <th>Race time</th>
                <th>Nominated time</th>
                <th>Predicted time</th>
                <th>&#x1D6AB;<sub>PN</sub></th>
                <th>&#x1D6AB;<sub>AN</sub></th>
            </tr>
            <tr ng-repeat="runner in runners">
                <td>{{ runner.name }}</td>
                <td class="time time-live" data-time="{{ runner.startTime.getTime() }}"></td>
                <td class="time">{{ runner.nominatedDuration | displayTime }}</td>
                <td class="time">{{ runner.getPredictedDuration() | displayTime }}</td>
                <td class="time">{{ runner.getDeltaPrediction() | displayTime }}</td>
                <td class="time">{{ runner.getDeltaActual() | displayTime }}</td>
            </tr>
        </table>
    </div>
</div>